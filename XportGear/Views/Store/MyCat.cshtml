
@model IEnumerable<XportGear.Models.Category>
@{
    ViewBag.Title = "Store";
}
@*<h3>Browse Genres</h3>*@
<p>
    Select from @Model.Count() genres:
</p>
<ul>
    @foreach (var catagorie in Model)
    {
        <li>@Html.ActionLink(catagorie.Category_Name, "Browse", new { catagorie = catagorie.Category_Name })</li>
    }
</ul>
